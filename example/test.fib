# Setup Registers
  # String starting point
  mode rA uint64

  # itterator value
  mode rB uint64

  # Length of the string
  mode rC uint64

  # The character being written to the string
  mode rD uint8

  # Where the current character will be placed
  mode rE uint64


# Construct string
  # Define the length of the string
  set rC 2
  
  # Allocate space for string
  mem alloc rA rC
  clone rA rE

  # Set itterator
  set rB 1

  # Build string from chars
  set rD 41
  push rD rE
  math rA add rB rE

  push rD rE


# Print the constructed string
  ss out rA rC


# Cleanup (optional)
  mem unalloc rA rC